{
  "name": "express-todo-app",
  "version": "0.0.0",
  "engines": {
    "node": "12.2.0"
  },
  "private": true,
  "scripts": {
    "start": "npm run workbox && nodemon bin/www.js",
    "dev": "nodemon bin/www.js",
    "workbox": "workbox injectManifest",
    "test": "nyc --reporter=html npm run test:unit --recursive --exit",
    "test:unit": "mocha --require ./test/app.js './{,!(node_modules)/**}/*.test.js'",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/coveralls/bin/coveralls",
    "codeclimate": "codeclimate-test-reporter < lcov.info",
    "coverage": "npm run coveralls && npm run codeclimate"
  },
  "dependencies": {
    "async": "^3.0.1",
    "compression": "^1.7.4",
    "connect-mongo": "^3.0.0",
    "debug": "~2.6.9",
    "directory-structure-json": "^1.0.0",
    "dotenv": "^8.0.0",
    "email-templates": "^6.0.0",
    "express": "~4.16.1",
    "express-session": "^1.16.2",
    "express-validator": "^5.3.1",
    "helmet": "^3.18.0",
    "http-errors": "~1.6.3",
    "https-localhost": "^4.4.1",
    "moment": "^2.24.0",
    "mongoose": "^5.6.0",
    "morgan": "~1.9.1",
    "nodemailer": "^6.2.1",
    "pug": "^2.0.4",
    "web-push": "^3.3.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codeclimate-test-reporter": "^0.5.1",
    "coveralls": "^3.0.4",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "workbox-cli": "^4.3.1"
  }
}
